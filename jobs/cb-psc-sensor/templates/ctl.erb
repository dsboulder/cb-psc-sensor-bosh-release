#!/bin/bash
#

set -e
RETVAL=0

case "$1" in
    start)
        echo "Starting cbagentd"
        systemctl start cbagentd.service
        echo "${NAME}."
        ;;
    stop)
        echo "Stopping cbagentd"
        systemctl stop cbagentd.service
        echo "${NAME}."
        ;;
    *)
        echo "Usage: $0 {start|stop}" >&2
        RETVAL=1
        ;;
esac

exit "${RETVAL}"

